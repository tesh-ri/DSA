int main() {
    int arr[]={4, 3, 2, 1 ,0 };
    int minIndex=0;
    
    //insertion sort
    for(int i=1; i<5; i++){
        for(int j=i; j>0;j--){
            if(arr[j]<arr[j-1]){
                swap(arr[j] , arr[j-1]);
            }
            else{
                break;
            }
        }
    }
    
    
    
    
    
    
